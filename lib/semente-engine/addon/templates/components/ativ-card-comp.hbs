{{#if notTravada}}
{{#if (equalstr value 0)}}
{{#if data_ok}}
<div class="timeline__end-class">Fim do bloco</div>

<div class="timeline__date-block class__date-block">
  <span class="icon-calendar icon--xl"></span>
  <span class="class__date">
    <span class="class__day-aux">Liberada desde</span>
    <span class="class__day">{{dia}} {{mensagem_mes}}</span>
  </span>
</div>

<div id="card_{{atividade.idx}}" class="timeline__class class class--is-completed">
  <span class="timeline__status">
    <span class="icon-unlocked"></span>
  </span>

  <div class="class__info flow-md">

    <div>
      <div class="class__subtitle title-aux">Aula {{atividade.idx}}</div>
      <h3 class="class__title">{{atividade.name}}</h3>
    </div>

    <div class="class__skills skills flow-xs">
      <h2 class="title-aux">Competências alvo</h2>
      <ul class="skills__list flow-xxs">
        {{#each atividade.aula.competencias as |c|}}
        <li>{{c.name}}</li>
        {{/each}}
      </ul>
    </div>

    <p class="class__description">{{atividade.description}}</p>
    <button class='class__cta btn btn--sm' {{action 'transitToAtividade' atividade.Id}}>Iniciar</button>
  </div>

  <span class="class__img" style="{{atividade.backgroundImage}}"></span>
</div>
{{else}}
<div class="timeline__end-class">Fim do bloco</div>

<div class="timeline__date-block class__date-block">
  <span class="icon-calendar icon--xl"></span>
  <span class="class__date">
    <span class="class__day-aux">Liberada a partir de</span>

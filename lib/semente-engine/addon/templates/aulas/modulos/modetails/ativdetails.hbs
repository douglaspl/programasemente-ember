<section class="course {{if menuIsOpen 'course--nav-is-show'}}" id="ativdetails_main_div">
  <header id="course-nav" class="course__nav course-nav">
    <div class="course-nav__title-menu-block">

      <button class="course-nav__menu-toggle" {{action 'toggleMenuStatus'}}>
        <span class="course-nav__icon-menu icon-menu"></span>
        <span class="course-nav__icon-close icon-x"></span>
      </button>

      <h1 class="course-nav__course-title">{{model.modulo.name}}</h1>

      {{#if (equalstr role 'aluno')}}
      {{#link-to 'modulos.modlist' model.modulo.id class="course-nav__back"}}
      <span class="icon-chevron-left"></span>
      Voltar para aulas
      {{/link-to}}
      {{else}}
      {{#link-to 'modulos.modlist' model.modulo.id class="course-nav__back"}}
      <span class="icon-chevron-left"></span>
      Voltar para aulas
      {{/link-to}}
      {{/if}}

    </div>

    <span class="stealth-info-navbar">{{nomeAluno}}</span>

    <!-- Marca da instituição -->

    <!-- Horizontal -->
    {{#if (equalstr isBradesco 9)}}
    <img class="course-nav__logo" src="data:image/jpeg;base64,">

    <!-- Vertical -->
    {{!--<img class="course-nav__logo" src="https://s3.amazonaws.com/gatry-static/promocao/imagem/481f035cbc2e9b352810f8e66da719dc.png">--}}
    {{/if}}

    <div class="course-nav__class">
      <span class="course-nav__class-order">Aula {{model.idx}}</span>
      <h2 class="course-nav__class-title">{{model.name}}</h2>
    </div>
    <nav id="scroller_menu" class="course-nav__nav">
      <ul class="course-nav__nodes">
        {{#each sortedSecoes as |secao|}}
          {{#if (isinarray secao secoes_enabled)}}
            {{#each secao.conteudos as |conteudo|}}
              {{#if conteudo.quiz}}
                {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id)
                model=model secao=secao conteudo=conteudo}}
                {{#each conteudo.quiz.questoes as |questao|}}
                  {{#if (isinarray questao questoes_enabled)}}
                    {{#if (isinarray questao questoes_respondidas)}}
                      {{section-link class='course-nav__node course-nav__node--is-done' elementId=(concatenate "li_conteudo_"
                      secao.id "_" conteudo.id "_" questao.idx) model=model secao=secao conteudo=conteudo questao=questao}}
                    {{else}}
                      {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id "_"
                      questao.idx) model=model secao=secao conteudo=conteudo questao=questao}}
                    {{/if}}
                  {{else}}
                    {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id "_"
                    questao.idx) model=model secao=secao conteudo=conteudo questao=questao disabled='disabled'}}
                  {{/if}}
                {{/each}}
              {{else}}
                {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id)
                model=model secao=secao conteudo=conteudo}}
              {{/if}}
            {{/each}}
          {{else}}
            {{#each secao.conteudos as |conteudo|}}
              {{#if conteudo.quiz}}
                {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id)
                model=model secao=secao conteudo=conteudo disabled='disabled'}}
                {{#each conteudo.quiz.questoes as |questao|}}
                  {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id "_"
                  questao.idx) model=model secao=secao conteudo=conteudo questao=questao disabled='disabled'}}
                {{/each}}
              {{else}}
                {{section-link class='course-nav__node' elementId=(concatenate "li_conteudo_" secao.id "_" conteudo.id)
                model=model secao=secao conteudo=conteudo disabled='disabled'}}
              {{/if}}
            {{/each}}
          {{/if}}
        {{/each}}
      </ul>
    </nav>
  </header>
  {{model.acompanhamentosCursoInstituicao}}
  {{outlet}}





</section>
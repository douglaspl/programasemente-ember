<!-- INÍCIO DE TODOS EM UMA INSTITUIÇÃO -->
<section class="report__section report-people report__section--is-active" id="report-people">

  <!-- INÍCIO DO ALERTA PARA TOTAL DE USUÁRIOS MARCADOS PARA COMPARAÇÃO -->
  <div class="report-people__selected-users alert alert--blue-navy-50" id="report-people-compare">
    <p>Você está com <strong>{{ap_length}} usuários</strong> selecionados.</p>
    <div class="alert__footer">
      <button class="btn btn--text" {{action 'deselectPessoaCompare'}}>Desmarcar todos</button>
      <button class="btn" {{action 'showComparison'}}>Comparar</button>
    </div>
  </div>
  <!-- FIM DO ALERTA PARA TOTAL DE USUÁRIOS MARCADOS PARA COMPARAÇÃO -->



  <div class="filter-list">

    <div class="filter-list__filter">

      <div class="form-group form-group--vertical w100">
        <label class="form-group__label" for="search">Busca</label>

        <form class="d-flex d-flex--c" id="searchAllEnrolled" autocomplete="off" aria-autocomplete="none">

          <div class="form-group__wrapper form-group__wrapper--search m-right--none" flex="100">
            {{input class="form-group__input" aria-describedby="basic-addon1" id="search_input_pessoas_adm"
            key-up="pagedsearch" type="text" autocomplete="off" aria-autocomplete="none"}}
          </div>

          <div class="m-top--sm">
            {{!-- <button class="btn a-self--fs" {{action 'pagedsearch'}}>Buscar</button> --}}
            <button class="btn btn--text btn--xs a-self--fs" {{action 'exitpagedsearch'}}>
              <span class="btn__icon icon icon-x"></span>Limpar</button>
          </div>

        </form>

      </div>

      <!-- ano e turma -->
      <div class="d-flex m-top--lg d--none animated fadeIn faster" id="year-class-filter">

        <div class="form-group form-group--vertical" flex="1">
          <span class="form-group__label">Ano</span>
          <div class="d-flex">
            {{#each inst_selected.modulos as |mod|}}
            <span class="checkbox m-right--lg">
              <input class="checkbox__input" id="ano_{{mod.id}}" name="year" type="checkbox" value="year{{mod.id}}"
                {{action 'filterano' mod.id on='change'}}>
              <label class="checkbox__label" for="ano_{{mod.id}}">{{mod.name}}</label>
            </span>
            {{/each}}
          </div>
        </div>

        <div class="form-group form-group--vertical form-group--select-wrapper" flex="1">
          <label class="form-group__label">Turma</label>

          <select id="turma_selector" class="form-group__input form-group__input--select" {{action 'filterTurma' on='change'}}>
            <option value="0" selected>Escolha uma turma</option>
            {{#if (equalstr pessoaLogged.role "instrutor")}}
              {{#each pessoaLogged.turmas as |turma|}}
                <option id="opt_{{turma.id}}" style="display: block" value={{turma.id}}>{{turma.name}}</option>
              {{/each}}
            {{else}}
              {{#each inst_selected.turmas as |turma|}}
                <option id="opt_{{turma.id}}" style="display: block" value={{turma.id}}>{{turma.name}}</option>
              {{/each}}
            {{/if}}
          </select>
        </div>

      </div>

      <div class="d-flex d-flex-justify--center m-top--lg">
        <button class="btn btn--xxs btn--is-toggle" {{action toggle 'year-class-filter'}}
          id="year-class-toggle" data-target="year-class-filter" style="height: 2rem; width: 9rem;">
          <span class="btn__label label__default">Mostrar mais</span>
          <span class="btn__label label__active">Mostrar menos</span>
        </button>
      </div>



    </div>

    <!-- ========================
        ########### LIST ############
        ========================= -->
    <div class="filter-list__list">

      <div class="total-results">
        <span class="total-results__results">Total de <strong>{{totalPessoas}}</strong> pessoas</span>
      </div>

      <div class="filter-list__ordering">
        <!-- ========================
                ######### ORDERING ##########
                ========================= -->

        <div class="form-group form-group--vertical">
          <label class="form-group__label" for="amount">Exibir</label>
          <div class="d-flex d-flex-align--center a-self--fs" flex-align="center">

            <div class="form-group--select-wrapper m-right--md">
              <select id="amount" class="form-group__input form-group__input--select form-group__input--amount"
                name="order" {{action 'setExibir' on='change'}}>
                {{#if (equalstr perPage 5)}}
                  <option value="5" selected>5</option>
                  <option value="10">10</option>
                  <option value="25">25</option>
                  <option value="50">50</option>
                  <option value="100">100</option>
                  <option value="500">500</option>
                  <option value="1000">1000</option>
                {{else if (equalstr perPage 25)}}
                  <option value="5">5</option>
                  <option value="10">10</option>
                  <option value="25" selected>25</option>
                  <option value="50">50</option>
                  <option value="100">100</option>
                  <option value="500">500</option>
                  <option value="1000">1000</option>
                {{else if (equalstr perPage 50)}}
                  <option value="5">5</option>
                  <option value="10">10</option>
                  <option value="25">25</option>
                  <option value="50" selected>50</option>
                  <option value="100">100</option>
                  <option value="500">500</option>
                  <option value="1000">1000</option>
                {{else if (equalstr perPage 100)}}
                  <option value="5">5</option>
                  <option value="10">10</option>
                  <option value="25">25</option>
                  <option value="50">50</option>
                  <option value="100" selected>100</option>
                  <option value="500">500</option>
                  <option value="1000">1000</option>
                {{else if (equalstr perPage 500)}}
                  <option value="5">5</option>
                  <option value="10">10</option>
                  <option value="25">25</option>
                  <option value="50">50</option>
                  <option value="100">100</option>
                  <option value="500" selected>500</option>
                  <option value="1000">1000</option>
                {{else if (equalstr perPage 1000)}}
                  <option value="5">5</option>
                  <option value="10">10</option>
                  <option value="25">25</option>
                  <option value="50">50</option>
                  <option value="100">100</option>
                  <option value="500">500</option>
                  <option value="1000" selected>1000</option>
                {{else}}
                  <option value="5">5</option>
                  <option value="10" selected>10</option>
                  <option value="25">25</option>
                  <option value="50">50</option>
                  <option value="100">100</option>
                  <option value="500">500</option>
                  <option value="1000">1000</option>
                {{/if}}
              </select>
            </div>
            <span>resultados por página</span>
          </div>
        </div>

        <div class="form-group form-group--vertical m-left--xxl">
          <label class="form-group__label" for="order">Ordenação</label>
          <div class="form-group--select-wrapper">
            <select id="sort_pessoas" class="form-group__input form-group__input--select" name="order"
              {{action 'ordenaPessoas' on='change'}}>
              {{#if (equalstr ordem 'completeness')}}
                <option value="az">Ordem alfabética A-Z</option>
                <option value="access">Acessos</option>
                <option value="completeness" selected>Completude</option>
              {{else if (equalstr ordem 'access')}}
                <option value="az">Ordem alfabética A-Z</option>
                <option value="access" selected>Acessos</option>
                <option value="completeness">Completude</option>
              {{else}}
                <option value="az" selected>Ordem alfabética A-Z</option>
                <option value="access">Acessos</option>
                <option value="completeness">Completude</option>
              {{/if}}
            </select>
          </div>
        </div>

        {{page-numbers content=pagedContent perPage=perPage class="m-left--auto pagination--center"}}
      </div>

      <div class="loader-local {{if (equalstr admdataLoaderState 1) 'loader-local--is-show' ''}}">

        <div class="loader-local__loader">
          <span class="icon icon-reload icon--xl"></span>
          Buscando
        </div>
        <ul class="report-people__list">
          {{#each pagedContent.content as |pessoa|}}
            <li class="report-people__person augmented-link">
              <a href="#" {{action 'selectPessoa' pessoa.id}}></a>
              {{!-- <div class="report-people__check form-group">
                            <span class="checkbox">
                                <input class="checkbox__input ckb_pessoa_cmp" id="personID_{{pessoa.id}}" name="remember" type="checkbox" value="personID_{{index}}" {{action 'selectPessoaCompare' pessoa.id preventDefault=false}}>
                                <label class="checkbox__label" for="personID_{{pessoa.id}}"></label>
                            </span>
                        </div> --}}
              <span class="report-people__avatar avatar avatar--lg">
                {{#if pessoa.uriAvatar}}
                  <img class="avatar__img" src={{pessoa.uriAvatar}} alt="{{pessoa.name}}">
                {{else}}
                  <img class="avatar__img" src="/assets/img/avatar-default.svg" alt="{{pessoa.name}}">
                {{/if}}
              </span>
              <div class="person">
                <span class="person__name">
                  {{pessoa.name}}
                </span>
                {{!-- <span class="person__email">{{pessoa.email}}</span> --}}
                {{!-- <span class="person__other">Último acesso: <strong>{{pessoa.lastAccess}}</strong></span> --}}
                {{!-- <span class="person__other">Área: <strong>{{pessoa.area.name}}</strong></span> --}}
              </div>
              {{!-- <div class="report-people__data">
                            <div class="data">
                                <span class="data__value">{{pessoa.nracessos}}</span>
                                <span class="data__label">acessos</span>
                            </div>
                            <div class="data data--md">
                                <span class="data__value">{{pessoa.cursosConcluidos}} de {{pessoa.numeroCursos}}</span>
                                <span class="data__label">cursos concluídos</span>
                            </div>
                        </div> --}}
            </li>
          {{/each}}
        </ul>
      </div>
      {{page-numbers content=pagedContent perPage=perPage class="pagination--right"}}

    </div>

  </div>

</section>

<!-- INÍCIO DA SESSÃO DE PESSOA ESPECÍFICA -->
<section style="display: none;" id="report-person-specific" class="report__person-specific">

  <button id="back-person-specific_old" class="btn btn--text" {{action 'closeEspecifico'}}>
    <span class="icon-arrow-left"></span>voltar
  </button>
  {{#if selected_pessoa}}
    <header class="report-person__header person">
      <div class="avatar avatar--xl">
        {{#if selected_pessoa.pessoa.uriAvatar}}
          <img src={{selected_pessoa.pessoa.uriAvatar}}>
        {{else}}
          <img src="/assets/img/avatar-default.svg">
        {{/if}}
      </div>

      <div>
        {{selected_pessoa.lastAccess}}
        <h1 class="title-1">{{selected_pessoa.pessoa.name}}</h1>
        <div>
          <span class="person__email title-aux">{{selected_pessoa.pessoa.email}}</span>
        </div>
        <span class="d-i-block popover">
          <button class="btn btn--text btn--xxs popover__trigger">mais informações</button>

          <div class="popover__balloon popover__balloon--lemon popover__balloon--right-bottom about-author">
            <button class="popover__close icon-x"></button>
            <ul class="">
              <li class="m-bottom--xs">Nascimento: <strong>01/01/1980</strong></li>
              <li class="m-bottom--xs">Sexo: <strong>Feminino/Masculino</strong></li>
              <li class="">Matrícula: <strong>200057258-3738</strong></li>
            </ul>
          </div>
        </span>
      </div>

      <ul class="person__info m-left--auto">
        <li class="person__other">Cadastrado em: <strong>{{selected_pessoa.cadastrado}}</strong></li>
        <li class="person__other">Último acesso: <strong>{{selected_pessoa.pessoa.lastAccess}}</strong></li>
        <li class="person__other">Total de acessos: <strong>{{selected_pessoa.pessoa.nracessos}}</strong></li>
      </ul>

    </header>

    {{#if selected_pessoa.matriculado}}
      <section class="report-person__body">
        <div class="form-group form-group--select-wrapper">
          {{moduloPercent}}
          <label class="form-group__label" for="select_course">Curso</label>
          <select class="form-group__input form-group__input--select" {{action 'selectCurso' on="change"}}
            id="select_course_pessoa" value="select_inst">
            {{!-- {{#if curso_selected}}
        <option value="none" style="font-style: italic;">Selecione o curso...</option>
        {{else}}
        <option value="none" selected style="font-style: italic;">Selecione o curso...</option>
        {{/if}} --}}
            {{#each selected_pessoa.modulos as |modulo|}}
              {{#if (equalstr curso_selected.id modulo.id)}}
                <option value={{modulo.id}} selected>{{modulo.name}} ({{modulo.progressPercentReal}} completo)</option>
              {{else}}
                <option value={{modulo.id}}>{{modulo.name}} ({{modulo.progressPercentReal}} completo)</option>
              {{/if}}
            {{/each}}
          </select>
        </div>
        {{#if curso_selected}}
          <ul>
            {{#each curso_selected.atividades as |atividade|}}
              <li class="report-person__class">
                <div class="d-flex d-flex--c d-flex-justify--center m-bottom--sm">
                  <span class="title-aux">Aula {{atividade.idx}}</span>
                  <h3 class="title-2">{{atividade.name}}</h3>
                </div>
                <ul class="report-class__progress">
                  {{#each atividade.secoes as |secao|}}
                    {{#if (equalstr secao.conteudo.type 'html')}}

                      {{#if (equalstr secao.conteudo.percent '100')}}
                        <li class="data data--bordered data--success data--short">
                          <span class="icon-check icon--success icon--lg"></span>
                          <span class="data__label">{{secao.nome}}</span>
                        </li>
                      {{else}}
                        <li class="data data--bordered data--light data--short">
                          <span class="icon-minus icon--lg"></span>
                          <span class="data__label">{{secao.nome}}</span>
                        </li>
                      {{/if}}

                    {{else if (equalstr secao.conteudo.type 'video')}}

                      {{#if (equalstr secao.conteudo.percent '100')}}
                        <li class="data data--bordered data--success data--short">
                          <span class="icon-check icon--success icon--lg"></span>
                          <span class="data__label">{{secao.nome}}</span>
                        </li>
                      {{else}}
                        <li class="data data--bordered data--light data--short">
                          <span class="icon-minus icon--lg"></span>
                          <span class="data__label">{{secao.nome}}</span>
                        </li>
                      {{/if}}

                    {{else}}

                      {{#if secao.conteudo.respondidas}}
                        <li class="data data--bordered data--success data--short">
                          <span class="data__value">{{secao.conteudo.aproveitamento}}<span class="small">%</span></span>
                          <span class="data__label">{{secao.nome}}</span>
                          {{!-- <span class="data__label">aproveitamento</span> --}}
                        </li>
                      {{else}}
                        <li class="data data--bordered data--light data--short">
                          <span class="icon-minus icon--lg"></span>
                          <span class="data__label">{{secao.nome}}</span>
                          {{!-- <span class="data__label">aproveitamento</span> --}}
                        </li>
                      {{/if}}

                    {{/if}}

                  {{/each}}
                </ul>

                <h4 class="report-class__subtitle">Dados dos quizes</h4>
                {{#each atividade.secoes as |secao|}}
                  {{#if (equalstr secao.conteudo.type 'quiz')}}
                    <table class="report-class__table table table--striped">
                      <thead>
                        <tr class="table__subheader">
                          <th class="report-class__main-info">{{secao.nome}}</th>
                          <th class="report-class__correct">acertou</th>
                          <th class="report-class__visualization">% visualização do feedback</th>
                          <th class="report-class__feedback">quantidade de vezes que o feedback foi reassistido</th>
                          <th class="report-class__time-spent">tempo de permanência na questão</th>
                        </tr>
                      </thead>
                      <tbody>
                        {{#each secao.conteudo.questoes as |questao|}}
                          <tr>
                            <td class="report-class__main-info">Questão {{questao.idx}}</td>
                            {{#if questao.respondida}}
                              {{#if questao.acertou}}
                                <td class="report-class__correct"><span class="icon-check icon--success"></span></td>
                              {{else}}
                                <td class="report-class__correct"><span class="icon-x icon--error"></span></td>
                              {{/if}}
                            {{else}}
                              <td class="report-class__correct"><span class="icon-minus"></span></td>
                            {{/if}}
                            <td class="report-class__visualization">{{questao.visfeedback}}%</td>
                            <td class="report-class__feedback">{{questao.qtdefeedback}}</td>
                            <td class="report-class__time-spent">{{questao.tempoquestaoalias}}</td>
                          </tr>
                        {{/each}}
                      </tbody>
                    </table>
                  {{/if}}
                {{/each}}
              </li>
            {{/each}}
          </ul>
        {{/if}}
      </section>
    {{else}}
      <!-- INÍCIO DO CASO 0, O USUÁRIO SEM MATRÍCULAS -->
      <div class="empty">
        <span class="empty__icon icon-frown-o icon--xxl"></span>
        Esse usuário não está matriculado em nenhum curso.
      </div>
      <!-- FIM DO CASO 0, O USUÁRIO SEM MATRÍCULAS -->
    {{/if}}
  {{else}}
    <div class="loader" id="firstAccessLoader">
      <div class="loader__circle">
        <img src="/assets/img/logo__programa-semente.jpg">
